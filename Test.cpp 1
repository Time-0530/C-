#define _CRT_SECURE_NO_WARNINGS

#include<iostream>
#include"string.h"

using namespace std;

void test1()
{
	String s1("hello world!");
	cout << s1.c_str() << endl;
	String s2(s1);
	cout << s2.c_str() << endl;

	//循环遍历
	for (size_t i = 0; i < s1.c_size(); i++)
	{
		cout << s1[i];
	}
	cout << endl;
	//迭代器
	auto it = s1.begin();
  //String::iterator it = s1.begin();
	while (it != s1.end())
	{
		cout << *it;
		++it;
	}
	cout << endl;
	//范围for
	for (auto ch : s1)
	{
		cout << ch;
	}
	cout << endl;
	//范围for和迭代器在底层代码是相同的
}

int main()
{
	test1();
	return 0;
}
